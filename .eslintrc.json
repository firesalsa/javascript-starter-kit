{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "no-console": 1,
    "for-direction": 1,
    "getter-return": 2,
    "no-compare-neg-zero": 1,
    "no-cond-assign": 2,
    "no-constant-condition": 1,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 1,
    "no-empty-character-class": 2,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": 1,
    "no-extra-semi": 1,
    "no-func-assign": 2,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-obj-calls": 2,
    "no-prototype-builtins": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-template-curly-in-string": 1,
    "no-unexpected-multiline": 1,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "accessor-pairs": 1,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "class-methods-use-this": 2,
    "complexity": 2,
    "consistent-return": 0,
    "curly": 2,
    "default-case": 1,
    "dot-location": 1,
    "dot-notation": 1,
    "eqeqeq": 2,
    "guard-for-in": 1,
    "no-case-declarations": 1,
    "no-else-return": 2,
    "no-empty-function": 1,
    "no-empty-pattern": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 2,
    "no-extra-label": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-global-assign": 2,
    "no-implicit-coercion": 0,
    "no-implicit-globals": 2,
    "no-implied-eval": 0,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 0,
    "no-lone-blocks": 1,
    "no-loop-func": 2,
    "no-magic-numbers": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal": 0,
    "no-octal-escape": 0,
    "no-param-reassign": 1,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-properties": 1,
    "no-return-assign": 2,
    "no-return-await": 1,
    "no-script-url": 1,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 1,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-useless-escape": 1,
    "no-useless-return": 2,
    "no-void": 0,
    "no-warning-comments": 0,
    "no-with": 0,
    "prefer-promise-reject-errors": 1,
    "radix": 1,
    "vars-on-top": 2,
    "strict": 0,
    "init-declarations": 1,
    "no-delete-var": 1,
    "no-label-var": 2,
    "no-restricted-globals": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 1,
    "no-use-before-define": 1,
    "callback-return": 2,
    "global-require": 0,
    "handle-callback-err": 1,
    "no-buffer-constructor": 1,
    "no-mixed-requires": 1,
    "no-new-require": 1,
    "no-process-env": 0,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 1,
    "array-bracket-newline": 0,
    "array-bracket-spacing": 1,
    "array-element-newline": 0,
    "block-spacing": 1,
    "brace-style": 1,
    "camelcase": 2,
    "capitalized-comments": 0,
    "comma-dangle": 0,
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "consistent-this": 0,
    "eol-last": 1,
    "func-call-spacing": 0,
    "func-name-matching": 0,
    "func-names": 0,
    "func-style": 1,
    "function-paren-newline": 1,
    "id-blacklist": 0,
    "id-length": 0,
    "id-match": 0,
    "implicit-arrow-linebreak": 1,
    "indent": [1, 2],
    "key-spacing": 1,
    "keyword-spacing": 1,
    "line-comment-position": 0,
    "linebreak-style": 1,
    "lines-around-comment": 0,
    "lines-between-class-members": 0,
    "max-len": 0,
    "new-cap": 1,
    "new-parens": 2,
    "newline-per-chained-call": 0,
    "no-array-constructor": 0,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-inline-comments": 1,
    "no-lonely-if": 0,
    "no-mixed-operators": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 1,
    "no-negated-condition": 0,
    "no-trailing-spaces": 1,
    "object-curly-newline": 1,
    "object-curly-spacing": 1,
    "object-property-newline": 1,
    "quotes": 1,
    "arrow-body-style": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "constructor-super": 0,
    "generator-star-spacing": 0,
    "no-class-assign": 1,
    "no-confusing-arrow": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-var": 2,
    "no-useless-rename": 1,
    "prefer-const": 2,
    "prefer-destructuring": 0,
    "prefer-numeric-literals": 0
  }
}
